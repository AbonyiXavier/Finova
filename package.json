{
  "name": "qred",
  "version": "1.0.0",
  "description": "qred challenge",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:dev": "ts-node-dev src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generates": "npm run typeorm  migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbonyiXavier/qred-challenge"
  },
  "author": "Francis Abonyi",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cron": "^2.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.10.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json}": "prettier --write",
    "src/**/*{js,ts,json}": "eslint"
  }
}
